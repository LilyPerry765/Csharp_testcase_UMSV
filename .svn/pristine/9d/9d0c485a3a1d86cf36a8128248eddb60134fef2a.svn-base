*
{
    margin: 0;
    padding: 0;
    text-decoration: none;
    list-style-type: none;
}
a
{
    color: black;
}
a:hover
{
    color: Red;
    text-decoration: underline;
}
img
{
    border-style: none;
}
body
{
    direction: rtl;
    font-family: Tahoma;
    text-align: center;
    color: #000;
    background: #fff;
    background-color: #EBF3FA;
    margin: 0;
}
#container
{
    width: 100%;
    margin: 20px auto;
}
.container-inner
{
    width: 100%;
}
td.header-leftlinks
{
    font-size: 9pt;
    text-align: left;
    vertical-align: top;
    padding-left: 5px;
}
.header-logo
{
    text-align: right;
    vertical-align: middle;
    padding-bottom: 5px;
}
.header-bar
{
    border-top: 3px solid #0065b5;
    background-color: #d6deec;
    vertical-align: middle;
    text-align: right;
    font-size: 8pt;
}
.header-baritems
{
    display: block;
    margin: 5px 5px 7px 0;
}
td.header-menu
{
    vertical-align: bottom;
}
.tab
{
    font-size: 8pt;
    display: block;
    float: right;
    margin: 0px 0px 1px 5px;
    padding: 3px 6px 4px 6px;
    border: 1px gray solid;
}
.tab-side
{
    font-size: 8pt;
    display: block;
    float: left;
}
.footer
{
    height: 30px;
    font-size: 8pt;
    border-top-style: solid;
    border-width: 1px;
    border-color: #0065B5;
    text-align: center;
}

.contents-td
{
    vertical-align: top;
}
#contents
{
    width: 100%;
    height: 400px;
    margin: 10px 0 10px 0;
    font-size: 9pt;
}
#tasks
{
    width: 200px;
    vertical-align: top;
    border: 1px solid #c1c1c1;
    background-color: #f6f6f6;
}
#contents #space
{
    width: 30px;
    vertical-align: top;
}
#contents #main
{
    width: 770px;
    vertical-align: top;
}
#tasks .task-item
{
    width: 184px;
    text-align: right;
    border: 1px solid #e7e7e7;
    background-color: #fff;
    margin: 8px auto;
}
#tasks .task-item th
{
    border: 1px solid #e7e7e7;
    background-color: #e7e7e7;
    text-align: center;
    height: 24px;
    font-weight: normal;
}
#tasks .task-item td
{
    height: 18px;
    padding-right: 3px;
}
#tasks .task-item td.spacer
{
    height: 10px;
}
#tasks .task-item .search-text
{
    width: 168px;
    height: 18px;
    line-height: 16px;
    padding: 1px;
}
#tasks .task-item .search-button
{
    margin-left: 3px;
    margin-top: 3px;
    padding: 2px;
}
#body
{
    width: 100%;
    border: 1px solid #c1c1c1;
    background-color: #fff;
    float: right;
}
#body-items
{
    width: 100%;
    margin: 0 auto;
}
#options
{
    width: 770px;
    border: 1px solid #c1c1c1;
    background-color: #fff;
    padding-bottom: 20px;
    float: right;
}
legend
{
    margin-right: 25px;
    padding: 4px 15px 8px 15px;
    background-color: #e7e7e7;
}
#items
{
    margin: 20px;
}
.item
{
    width: 156px;
    height: 32px;
    display: block;
    float: right;
    font-size: 9pt;
    margin: 10px;
    cursor: hand;
}
.item img
{
    float: right;
    height: 32px;
    width: 32px;
}
.item span
{
    margin-top: 11px;
    margin-right: 15px;
    float: right;
}
table .datalist
{
    width: 725px;
    border-width: 0px;
    table-layout: auto;
    border-collapse: collapse;
    border-spacing: 0px;
}
.datalist th
{
    height: 19px;
    color: black;
    background-image: url('images/table-header-bg.gif');
    font-weight: normal;
    padding-right: 5px;
    padding-left: 5px;
}
.datalist td
{
    height: 22px;
    color: black;
}
.cell-id
{
    width: 35px;
}
.cell-title
{
    width: 340px;
}
.cell-number
{
    width: 80px;
}
.cell-edit
{
    width: 75px;
}
.cell-date
{
    width: 125px;
}
.ltr
{
    direction: ltr;
}
.rtl
{
    direction: rtl;
}
.left
{
    text-align: left;
}
.list-confirm-button
{
    box-shadow: -1px 2px 3px #336699;
    float: left;
    margin-bottom: 10px;
    margin-left: 5px;
    padding: 0px 10px 0px 10px;
}
.right
{
    text-align: right;
}
.center
{
    text-align: center;
    vertical-align: middle;
}
.invisible
{
    display: none;
}
.inactive
{
    color: #C0C0C0;
}
.top-border
{
    border-top: solid 1px #e7e7e7;
}
.current
{
    color: #0066CC;
    font-weight: bold;
    font-style: normal;
    text-decoration: underline;
}
.past
{
    color: #D78A8A;
    text-transform: none;
}
.future
{
    color: #009D9D;
    text-decoration: line-through;
}

.RowZero
{
    height: 490px;
    background: green;
}

