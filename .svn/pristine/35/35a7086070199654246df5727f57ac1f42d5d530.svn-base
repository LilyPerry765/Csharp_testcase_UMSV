.dialogwindow
{
    border: medium solid #5E86C4;
    display: none;
    position: absolute;
    background: white;
    width: 300px;
    height: 230px;
    -moz-border-radius: 15px;
    -webkit-border-radius: 15px;
    border-radius: 15px;
    -khtml-border-radius: 15px;
