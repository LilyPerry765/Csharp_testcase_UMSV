/* GENERAL
***********************************************************************************/
*
{
    font-family: Tahoma;
    font-size: 9pt;
    vertical-align: top;
    list-style-type: none;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
}
a
{
    color: #FFFFFF;
}
.gray_background
{
    background-color: #B9B9B9;
}
.right_label
{
    padding: 2px;
    margin: 2px;
    float: right;
    width: 58px;
}
.hidden, .print-only
{
    display: none;
    visibility: collapse;
}
/* LOGIN PAGE
***********************************************************************************/
.login_div
{
    border: medium solid #666666;
    background-color: #DDDEE6;
    vertical-align: middle;
    width: 400px;
    height: 200px;
    -moz-border-radius: 15px;
    -webkit-border-radius: 15px;
    border-radius: 15px;
    -khtml-border-radius: 15px;
    -o-border-radius: 15px;
    -icab-border-radius: 15px;
    text-align: center;
    padding-top: 10px;
    direction: rtl;
}
.login_button
{
    padding: 4px;
    margin: 14px 0px 0px 50px;
    width: 80px;
    float: left;
}
.shadow1
{
    background-color: rgb(102, 102, 102);
    -moz-box-shadow: 5px 5px 2px rgba(68,68,68,0.6);
    -webkit-box-shadow: 5px 5px 2px rgba(68,68,68,0.6);
    box-shadow: 5px 5px 2px rgba(68,68,68,0.6);
    filter: progid:DXImageTransform.Microsoft.Blur(PixelRadius=,MakeShadow=true,ShadowOpacity=0.30);
    -ms-filter: "progid:DXImageTransform.Microsoft.Blur(PixelRadius=3,MakeShadow=true,ShadowOpacity=0.30)";
    zoom: 1;
    width: 410px;
    height: 220px;
    margin-left: 3px;
    vertical-align: middle;
    text-align: center;
    position: absolute;
    top: 50%;
    margin-top: -120px;
    right: 50%;
    margin-right: -200px;
    -moz-border-radius: 15px;
    -webkit-border-radius: 15px;
    border-radius: 15px;
    -khtml-border-radius: 15px;
    -o-border-radius: 15px;
    -icab-border-radius: 15px;
}
.shadow1 .shadow2
{
    position: relative; /* This protects the inner element from being blurred */
    background-color: rgb(102, 102, 102);
    width: 400px;
    vertical-align: middle;
    text-align: center;
    -moz-border-radius: 15px;
    -webkit-border-radius: 15px;
    border-radius: 15px;
    -khtml-border-radius: 15px;
    -o-border-radius: 15px;
    -icab-border-radius: 15px;
}
.right_div
{
    float: right;
    text-align: center;
    vertical-align: middle;
    padding-right: 10px;
}
.left_div
{
    float: left;
    text-align: center;
    vertical-align: middle;
    width: 250px;
    height: 175px;
    margin-top: 10px;
    margin-right: 3px;
    margin-left: 18px;
}
.login_image
{
    margin: 20px 10px 10px 5px;
    vertical-align: middle;
    text-align: right;
    width: 90px;
    height: 100px;
}
.password_textbox
{
    width: 150px;
}
.login_checkbox
{
    margin: 8px 10px 0px 83px;
    float: right;
}
.login_error_label
{
    color: #FF3300;
    vertical-align: middle;
    float: right;
    margin-right: 12px;
    margin-top: 5px;
}

